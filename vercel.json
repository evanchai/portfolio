{
  "rewrites": [
    {
      "source": "/plat",
      "destination": "https://plat-ning.vercel.app"
    },
    {
      "source": "/plat/:path*",
      "destination": "https://plat-ning.vercel.app/:path*"
    },
    {
      "source": "/museo",
      "destination": "https://museo-ning.vercel.app"
    },
    {
      "source": "/museo/:path*",
      "destination": "https://museo-ning.vercel.app/:path*"
    },
    {
      "source": "/poker",
      "destination": "https://poker-ning.vercel.app"
    },
    {
      "source": "/poker/:path*",
      "destination": "https://poker-ning.vercel.app/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}
